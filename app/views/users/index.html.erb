<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<%@users.each do |a|%>
    <%= a.username%>
    <% a.posts.each do |p|%>
        <%if p.title != nil && p.body != nil%>
            <p>Title:</p>
                <%= p.title%>
            <p>Body:</p>
                <%= p.body%>
                <br>
        <%end%>
        <%= p.comments%>
    <%end%>
    <br>
<%end%>